L = []
for x in range(1, 11):
    L.append(x * x)

a=[x * x for x in range(1, 11)]
b=[x * x for x in range(1, 11) if x % 2 == 0]
c=[m + n for m in 'ABC' for n in 'XYZ']


import os
d=[d for d in os.listdir('.')]
e={'x': 'A', 'y': 'B', 'z': 'C' }
f=[k + '=' + v for k, v in d.items()]
g=['Hello', 'World', 'IBM', 'Apple']
h=[s.lower() for s in L]